import java.util.Arrays;
class HelloWorld {
    public static void main(String[] args) {
        int[] n = {7,2,10,3,4,5};
        int temp = 0;
        for(int i = 0; i < n.length-1; i++){
            for(int j = i+1; j < n.length-1;j++){
                if(n[i] > n[j]){
                    temp = n[i];
                    n[i] = n[j];
                }
            }
        }
        System.out.print(temp);
    }
}
     
